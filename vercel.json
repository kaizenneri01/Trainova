{
  "builds": [
    {
      "src": "apps/frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/frontend"
    }
  ],
  "buildCommand": "pnpm turbo run build --filter=@trainova/frontend",
  "outputDirectory": "apps/frontend/.next"
}